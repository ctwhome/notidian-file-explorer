{
	"name": "notidian-file-explorer",
	"version": "1.0.3",
	"description": "A multi-column file explorer for Obsidian, similar to macOS Finder's column view.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"bump": "node bump-version.mjs",
		"bump:minor": "node bump-version.mjs minor",
		"bump:major": "node bump-version.mjs major",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"emoji-picker-element": "^1.26.1",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	}
}
